@model RealWealth.Models.UserReports

@{
    ViewBag.Title = "PaidIncome";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<link href="~/plugins/flatpickr/flatpickr.css" rel="stylesheet" type="text/css">
<link href="~/plugins/flatpickr/custom-flatpickr.css" rel="stylesheet" type="text/css">
@using (Html.BeginForm("PaidIncome", "Report", FormMethod.Post, new { @id = "form1", enctype = "multipart/form-data" }))
{
    <div class="container">
        <div class="row layout-top-spacing">
            <div class="col-lg-12 col-12 layout-spacing">
                <div class="statbox widget box box-shadow">
                    <div class="widget-header">
                        <div class="row">
                            <div class="col-lg-4">
                                <div class="form-group">
                                    <h4>Paid Bonus</h4>
                                </div>
                            </div>
                            <div class="col-lg-8">
                                <div class="form-group" style="float:right">
                                    <h4> <a href="/Report/PayoutDetail" class="btn btn-info btn-sm">Back</a></h4>
                                </div>
                            </div>
                        </div>
                        <div class="widget-content widget-content-area">
                            <table id="html5-extension" class="table table-hover non-hover" data-page-length='20'>
                                <thead>
                                    <tr>
                                        <th>Sr No.</th>
                                        <th>Login Id</th>
                                        <th>From Name</th>
                                        <th>Level</th>
                                        <th>Business Amount</th>
                                        <th>BV</th>
                                        <th>Package</th>
                                        <th>Commision Amount</th>
                                        <th>(%)</th>
                                        <th>Transaction Date</th>
                                        @*<th>To Name</th>*@
                                        <th>Payout No</th>
                                        <th>Status</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @if (Model != null)
                                    {
                                        if (Model.lst != null)
                                        {
                                            int i = 1;
                                            foreach (var Item in Model.lst)
                                            {
                                    <tr class="text-left">
                                        <td>@i</td>
                                        <td>@Item.LoginId</td>
                                        <td>@Item.FromName</td>
                                        <td>@Item.Level</td>
                                        <td>@Item.BusinessAmount</td>
                                        <td>@Item.BV</td>
                                        <td>@Item.ProductName</td>
                                        <td>@Item.Amount</td>
                                        <td>@Item.CommissionPercentage %</td>
                                        <td>@Item.TransactionDate</td>
                                        @*<td>@Item.ToName</td>*@
                                        <td>@Item.PayoutNo</td>
                                        <td>@Item.Status</td>
                                    </tr>
                                                i++;
                                            }
                                        }
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



}
<script src="~/plugins/flatpickr/flatpickr.js"></script>
<script src="~/plugins/flatpickr/custom-flatpickr.js"></script>
<script src="~/RealWealthWebsite/assets/js/jquery.min.js"></script>
<script>
    var f3 = flatpickr(document.getElementById('FromDate'), {
        mode: "range"
    });
    var f4 = flatpickr(document.getElementById('ToDate'), {
        mode: "range"
    });
    $("#Status").val($("#st").val()).attr("selected", "selected");
</script>




