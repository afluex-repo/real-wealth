    function CreateOrder(Type) {
        debugger;
        $.ajax({
            url: '/Recharge/CreateOrder',
            data: { Amount: $("#" + Type + "Amount").val(), MobileNo: $("#" + Type + "No").val(), Type: Type },
            Type: "Get",
            DataType: "JSON",
            success: function (data) {
                debugger;
                if (data.Result == "Yes") {
                    if (confirm("Are you sure you want to continue for recharge?")) {
                        if (Type == "Prepaid" || Type == "DTH") {
                            PrepaidandDTHRecharge(data.OrderNo, Type);
                        }
                        else if (Type == "Postpaid") {
                            PostpaidRecharge(data.OrderNo, Type);
                        }
                        else {
                            UtilityPayments(data.OrderNo, Type);
                        }
                    }
                    else {
                        return false;
                    }
                }
                else if (data.Result == "No") {
                    alert(data.Message);
                }
            },
            error: function () {
                alert("Error");
            }
        });
    }
